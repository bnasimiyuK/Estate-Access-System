            "content": "            <!DOCTYPE html>\r\n            <html lang=\"en\">\r\n            <head>\r\n                <meta charset=\"UTF-8\">\r\n                <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">\r\n                <title>Dashboard Overview</title>\r\n\r\n                <script src=\"https://cdn.tailwindcss.com\"></script>\r\n\r\n                <script src=\"https://cdn.jsdelivr.net/npm/chart.js\"></script>\r\n            \r\n                <style>\r\n                    /* Ensures the chart has a consistent height */\r\n                    #accessChart {\r\n                        max-width: 100%;\r\n                        height: 384px; \r\n                    }\r\n                </style>\r\n            </head>\r\n\r\n            <body class=\"bg-gray-100 min-h-screen font-sans\">\r\n\r\n                <div class=\"p-6\">\r\n                    <h2 class=\"text-3xl font-bold text-gray-800 mb-6 border-b pb-3\">\r\n                        Administrative & Operations Overview\r\n                    </h2>\r\n                    \r\n                    <section class=\"grid grid-cols-1 md:grid-cols-4 gap-6 mb-8\">\r\n                        <div class=\"bg-white rounded-xl shadow-lg p-6 text-center border-b-4 border-blue-500\">\r\n                            <p class=\"text-gray-500 text-sm font-semibold\">Total Residents</p>\r\n                            <h2 id=\"totalResidents\" class=\"text-3xl font-extrabold text-blue-700 mt-1\">0</h2>\r\n                        </div>\r\n\r\n                        <div class=\"bg-white rounded-xl shadow-lg p-6 text-center border-b-4 border-yellow-500\">\r\n                            <p class=\"text-gray-500 text-sm font-semibold\">Pending Payments</p>\r\n                            <h2 id=\"pendingPayments\" class=\"text-3xl font-extrabold text-yellow-600 mt-1\">0</h2>\r\n                        </div>\r\n\r\n                        <div class=\"bg-white rounded-xl shadow-