            "content": "<!DOCTYPE html>\r\n<html lang=\"en\">\r\n<head>\r\n  <meta charset=\"UTF-8\" />\r\n  <meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />\r\n  <title>Membership Records | Admin Portal</title>\r\n  <script src=\"https://cdn.tailwindcss.com\" defer></script>\r\n  <link href=\"https://fonts.googleapis.com/css2?family=Poppins:wght@400;600&display=swap\" rel=\"stylesheet\" />\r\n</head>\r\n<body class=\"min-h-screen bg-gray-50 text-gray-800 font-[Poppins]\">\r\n\r\n  <!-- Navbar -->\r\n  <div id=\"header\" class=\"w-full bg-blue-900 text-white py-3 text-center\">\r\n    <h1 class=\"text-lg font-semibold\">Athi Estate Access Management System</h1>\r\n  </div>\r\n\r\n  <main class=\"p-8\">\r\n    <section class=\"mb-8\">\r\n      <h1 class=\"text-3xl font-semibold text-blue-800 mb-6 border-b-2 border-blue-500 pb-2\">\r\n        Membership Records\r\n      </h1>\r\n\r\n      <div id=\"msg\" class=\"text-sm text-gray-700 mb-3\"></div>\r\n\r\n      <div class=\"flex flex-wrap items-center justify-between mb-4 text-sm text-gray-700\">\r\n        <p>Total Membership Records: <span id=\"membershipCount\" class=\"font-bold text-blue-700\">0</span></p>\r\n        <p>Status: <span id=\"statusLabel\" class=\"font-semibold text-blue-600\">All</span></p>\r\n        <p>Last Update: <span id=\"actionText\" class=\"font-semibold text-green-600\">Loading...</span></p>\r\n      </div>\r\n\r\n      <!-- Filter Dropdowns -->\r\n      <div class=\"flex flex-wrap items-center gap-4 mb-4\">\r\n        <select id=\"requestIdFilter\" class=\"border border-gray-300 rounded px-3 py-1 focus:outline-none focus:ring focus:border-blue-500\">\r\n          <option value=\"\">All Request IDs</option>\r\n        </select>\r\n\r\n        <select id=\"residentFilter\" class=\"border border-gray-300 rounded px-3 py-1 focus:outline-none focus:ring focus:border-blue-500\">\r\n          <option value=\"\">All Residents</option>\r\n        </select>\r\n\r\n        <button id=\"clearFilterBtn\" class=\"bg-gray-500 text-white px-4 py-1 rounded hover:bg-gray-600\">\r\n          Clear Filters\r\n        </button>\r\n      </div>\r\n\r\n      <!-- Membership Table -->\r\n      <div class=\"bg-white rounded-xl border border-gray-300 shadow-md p-6 overflow-x-auto mb-6\">\r\n        <table class=\"w-full text-sm text-left\">\r\n          <thead>\r\n            <tr class=\"bg-blue-700 text-white\">\r\n              <th class=\"px-4 py-3\">RequestID</th>\r\n              <th class=\"px-4 py-3\">ResidentName</th>\r\n              <th class=\"px-4 py-3\">NationalID</th>\r\n              <th class=\"px-4 py-3\">PhoneNumber</th>\r\n              <th class=\"px-4 py-3\">Email</th>\r\n              <th class=\"px-4 py-3\">HouseNumber</th>\r\n              <th class=\"px-4 py-3\">CourtName</th>\r\n              <th class=\"px-4 py-3\">RoleName</th>\r\n              <th class=\"px-4 py-3\">Status</th>\r\n              <th class=\"px-4 py-3\">RequestedAt</th>\r\n               <th class=\"px-4 py-3\">ApprovedAt</th>\r\n              <th class=\"px-4 py-3 text-center\">Actions</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody id=\"membershipTableBody\" class=\"text-gray-700\"></tbody>\r\n        </table>\r\n      </div>\r\n\r\n      <!-- Approved Residents Table -->\r\n      <div class=\"bg-white rounded-xl border border-gray-300 shadow-md p-6 overflow-x-auto\">\r\n        <h2 class=\"text-xl font-semibold text-blue-800 mb-3\">Approved Residents</h2>\r\n        <table class=\"w-full text-sm text-left\">\r\n          <thead>\r\n            <tr class=\"bg-blue-700 text-white\">\r\n              <th class=\"px-4 py-3\">ResidentName</th>\r\n              <th class=\"px-4 py-3\">NationalID</th>\r\n              <th class=\"px-4 py-3\">PhoneNumber</th>\r\n              <th class=\"px-4 py-3\">Email</th>\r\n              <th class=\"px-4 py-3\">HouseNumber</th>\r\n              <th class=\"px-4 py-3\">CourtName</th>\r\n            </tr>\r\n          </thead>\r\n          <tbody id=\"residentsTableBody\" class=\"text-gray-700\"></tbody>\r\n        </table>\r\n      </div>\r\n\r\n    </section>\r\n\r\n    <footer class=\"mt-8 text-center text-gray-500 text-sm\">\r\n      &copy; <span id=\"year\"></span> Athi Highway Estate Access Management System\r\n    </footer>\r\n  </main>\r\n\r\n  <script src=\"js/main.js\" defer></script>\r\n  <script src=\"js/navbar.js\" defer></script>\r\n  <script src=\"js/membershiprecords.js\" defer></script>\r\n  <script>\r\n    document.getElementById(\"year\").textContent = new Date().getFullYear();\r\n  </script>\r\n</body>\r\n</html>\r\n"
